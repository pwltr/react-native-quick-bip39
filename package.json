{
  "name": "react-native-quick-bip39",
  "version": "0.0.7",
  "description": "React-Native fast bip39 using react-native-quick-crypto",
  "main": "dist/index.mjs",
  "scripts": {
    "tsc:check": "tsc -p tsconfig.json --noEmit",
    "format": "biome format --write ./src",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "pre-commit": "biome check ./src",
    "build": "tsup"
  },
  "author": "Philipp Walter",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pwltr/react-native-quick-bip39.git"
  },
  "license": "MIT",
  "dependencies": {
    "@craftzdog/react-native-buffer": "^6.0.5",
    "react-native-quick-crypto": "^0.7.7",
    "unorm": "^1.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@react-native/typescript-config": "^0.76.3",
    "@types/jest": "^29.5.14",
    "@types/react-native": "^0.73.0",
    "@types/unorm": "^1.3.28",
    "nyc": "^8.3.0",
    "proxyquire": "^1.7.10",
    "tape": "^4.6.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "bugs": {
    "url": "https://github.com/pwltr/react-native-quick-bip39/issues"
  },
  "homepage": "https://github.com/pwltr/react-native-quick-bip39#readme",
  "keywords": [
    "react-native",
    "bip39",
    "react-native-quick-crypto"
  ]
}
